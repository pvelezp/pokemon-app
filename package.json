    "@tanstack/react-query": "^4.29.3",
    "axios": "^1.3.6",
    "eslint": "^8.57.1",
    "glob": "^9.3.5",
    "lru-cache": "^11.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
